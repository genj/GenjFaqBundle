{
    "name":        "genj/faq-bundle",
    "type":        "symfony3-bundle",
    "description": "Symfony3 FAQ bundle",
    "keywords":    ["genj", "faq", "symfony", "bundle"],
    "homepage":    "http://github.com/genj/GenjFaqBundle",
    "license":     "MIT",

    "authors": [
        {
            "name":  "Choong Wei Tjeng",
            "email": "choong.wei.tjeng@genj.nl"
        },
        {
            "name":  "Nico Kaag",
            "email": "nico.kaag@genj.nl"
        },
        {
            "name":  "Susan Lau",
            "email": "susan.lau@genj.nl"
        }
    ],

    "require": {
        "symfony/symfony": ">=3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/orm": ">=2.5",
        "doctrine/doctrine-bundle": "~1.6",
        "gedmo/doctrine-extensions": ">=2.4.10"
    },

    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^2.7"
    },

    "config": {
        "preferred-install": "dist"
    },

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "Genj\\FaqBundle\\": "src/"
        }
    }
}
